    <iconUrl>https://raw.githubusercontent.com/ljw1004/opencv/vs2015-samples/vs2015/WS/10.0/NuPkg/opencv.uwp.png</iconUrl>
    <tags>opencv UWP</tags>
    <dependencies>
      <dependency id="OpenCV.UWP.native.flann" version="0.0.2-alpha" />
      <dependency id="OpenCV.UWP.native.imgproc" version="0.0.2-alpha" />
    </dependencies>
  </metadata>
</package>